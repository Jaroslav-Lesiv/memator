{
  "compilerOptions": {
    "target": "es5",
    "noEmit": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "jsx": "react-native",
    "module": "es2015",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "checkJs": true,
    "strict": false,
    "allowJs": true,
    "experimentalDecorators": true,
    "esModuleInterop": true
  },
  "extends": "expo/tsconfig.base"
}
